      <script>
        var titles = document.getElementsByClassName("skill-title")
        var contents = document.getElementsByClassName("skills")
    
        function display(skillset){
            for (title of titles){
                title.classList.remove("selected")
            }
    
            for (content of contents){
                if (!content.classList.contains("hide")){
                content.classList.add("hide")}
            }
            event.currentTarget.classList.add("selected")
            document.getElementById(skillset).classList.remove("hide")
    
        }
    
    </script>
    <script>
      const scriptURL = 'https://script.google.com/macros/s/AKfycbyYcQb8zKTEKTc3qdH3N5yE9zdKxM7kPAYnYikN2TTy22uck44vDQlQ99B8mRlvB8Dq8w/exec'
      const form = document.forms['submit-to-google-sheet']
      const msg = document.getElementById("confirmation")
    
      form.addEventListener('submit', e => {
        e.preventDefault()
        fetch(scriptURL, { method: 'POST', body: new FormData(form)})
          .then(response => {
              msg.innerHTML = "Message Sent Successfully!"
              setTimeout(function(){
                msg.innerHTML = ""
              }, 10000)
              form.reset()
            }
          )
          .catch(error => console.error('Error!', error.message))
      })
    </script>

    <script>
      const pictures = document.querySelectorAll(".photos")
      function setClass(){
        const classes = ["first", "second", "third"]
        pictures.forEach((picture, index) => {
          picture.classList.add(classes[index])
        })
      }
    
      function changePosition(){
        const firstImage = document.querySelector(".photos.first")
        const secondImage = document.querySelector(".photos.second")
        const thirdImage = document.querySelector(".photos.third")
    
        const firstImageClasses = firstImage.className
        const secondImageClasses = secondImage.className
        const thirdImageClasses = thirdImage.className
    
        firstImage.className = thirdImageClasses
        secondImage.className = firstImageClasses
        thirdImage.className = secondImageClasses
    
        pictures.forEach(picture => {
          picture.removeEventListener("click", changePosition)
          if(picture.classList.contains('first')) {
          picture.addEventListener("click", changePosition)    
        }
        })
      }
    
      setClass()
    
      pictures.forEach( picture => {
        if(picture.classList.contains('first')) {
          picture.addEventListener("click", changePosition)    
        }
      })
    </script>

